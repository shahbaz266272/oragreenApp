{
  "expo": {
    "name": "Oragreen",
    "slug": "oragreen",
    "version": "1.0.2",
    "orientation": "portrait",
    "userInterfaceStyle": "light",

    "icon": "./assets/splash.png",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#007bff"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.oragreen.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.oragreen.app"
    },

    "web": {},

    "extra": {
      "eas": {
        "projectId": "4e517f72-c9c2-4d23-a107-ea17eb60136b"
      }
    }
  }
}
